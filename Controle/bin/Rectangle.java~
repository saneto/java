import java.*;

public class Rectangle extends FormeGeometrique{
	double hauteur, largeur;
	
	public Rectangle(double x, double y, double h, double l){
		posX = x; posY = y; hauteur = h; largeur = l;
	}

	double surface(){	return hauteur*largeur;	}
	double perimetre(){	return (hauteur+largeur)*2;	}
}
